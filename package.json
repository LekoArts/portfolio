    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-lodash": "^3.0.5",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-netlify-cache": "^1.1.0",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-source-prismic": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.17",
    "lodash": "^4.17.11",
    "polished": "^3.0.3",
    "prismic-reactjs": "^0.3.2",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.4",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.0",
    "react-spring": "^8.0.15",
    "react-typography": "^0.16.19",
    "styled-components": "^4.1.3",
    "styled-theming": "^2.2.0",
    "typeface-istok-web": "^0.0.71",
    "typeface-montserrat": "^0.0.54",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "chrome-launcher": "^0.10.5",
    "cli-table": "^0.3.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.3.6",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-cypress": "^0.1.7",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lighthouse": "^4.2.0",
